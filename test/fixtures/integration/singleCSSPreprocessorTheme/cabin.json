{
  "CSSPreprocessor": [
    "Sass"
  ],
  "templateEngine": [
    "Jade",
    "EJS"
  ],
  "gruntPagesVersion": "~0.3.0",
  "gruntPagesConfig": {
    "options": {
      "pageSrc": "src/pages"
    },
    "posts": {
      "src": "posts",
      "dest": "dist",
      "layout": "src/layouts/post.<%= templateEngine %>",
      "url": "blog/posts/:title"
    }
  }
}
